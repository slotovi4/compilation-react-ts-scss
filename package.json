{
	"name": "compilation-react-ts-scss",
	"version": "1.1.1",
	"private": true,
	"dependencies": {
		"@bem-react/classname": "^1.5.11",
		"@rematch/core": "^2.0.1",
		"connected-react-router": "^6.9.1",
		"history": "^5.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.4",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.3",
		"redux": "^4.1.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "GENERATE_SOURCEMAP=false react-scripts build",
		"test": "react-scripts test --no-cache --coverage --watchAll=false",
		"eject": "react-scripts eject",
		"test-ci": "yarn test --ci",
		"lint-ci": "eslint ."
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@slotovi4/eslint-config": "^1.4.2",
		"@types/jest": "^27.0.0",
		"@types/node": "^16.4.14",
		"@types/react": "^17.0.17",
		"@types/react-dom": "^17.0.9",
		"@types/react-redux": "^7.1.18",
		"@types/react-router-dom": "^5.1.8",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-react-memo": "^0.0.3",
		"husky": "4.3.8",
		"lint-staged": "^11.1.2",
		"sass": "^1.37.5",
		"typescript": "^4.3.5",
		"yarn-upgrade-all": "^0.5.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --allow-empty",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"jest": {
		"coverageReporters": [
			"html",
			"text"
		],
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80,
				"statements": -10
			}
		},
		"collectCoverageFrom": [
			"**/*.{ts,tsx}",
			"!**/node_modules/**",
			"!**/*.d.ts"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{js,ts,tsx,jsx}": [
			"eslint --fix"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"yarn-upgrade-all": {
		"ignore": [
			"husky"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
